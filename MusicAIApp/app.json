{
  "expo": {
    "name": "MusicAIApp",
    "slug": "MusicAIApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSMicrophoneUsageDescription": "Gitarını ve çaldığın notaları duymak için mikrofon iznine ihtiyacımız var."
      },
      "bundleIdentifier": "com.anonymous.MusicAIApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "AUDIO_RECORDING",
        "RECORD_AUDIO"
      ],
      "package": "com.anonymous.MusicAIApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-av",
        {
          "microphonePermission": "Gitarını ve çaldığın notaları duymak için mikrofon iznine ihtiyacımız var."
        }
      ]
    ]
  }
}